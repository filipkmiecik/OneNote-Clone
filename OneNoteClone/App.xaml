<Application x:Class="OneNoteClone.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:OneNoteClone"
             StartupUri="View/NoteWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="TextColor" Color="#FFFFFF" />
        <SolidColorBrush x:Key="BackgroundDark" Color="#262626" />
        <SolidColorBrush x:Key="BackgroundMedium" Color="#404040" />
        <SolidColorBrush x:Key="BackgroundBright" Color="#666666" />
    </Application.Resources>
    
</Application>
